{
  "name": "gql-doctor",
  "version": "0.0.0",
  "description": "GraphQL Doctor",
  "license": "MIT",
  "scripts": {
    "start": "probot run ./dist",
    "build": "tsc",
    "tsc": "tsc --noEmit",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --exec \"yarn start\" --watch ./dist"
  },
  "dependencies": {
    "probot": "9.14.1"
  },
  "devDependencies": {
    "@types/node": "11.9.4",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "concurrently": "4.1.0",
    "eslint": "5.14.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "smee-client": "1.0.2",
    "typescript": "3.3.3"
  }
}
